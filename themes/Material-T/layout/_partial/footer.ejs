<footer class="mt-5">
  <div class="text-center py-3">
    <a href="https://hexo.io" target="_blank" rel="nofollow noopener"><b>HEXO</b></a>
    <i class="iconfont icon-love"></i>
    <a href="https://github.com/0x2e/Material-T" target="_blank" rel="nofollow noopener"> <b>Material-T</b></a>
    <br>
    <% if(theme.footer.beian.enable) { %>
      <a href="http://www.miitbeian.gov.cn/" target="_blank" rel="nofollow noopener"><%- theme.footer.beian.icp_text %></a>
      <% if(theme.footer.beian.police_text && theme.footer.beian.police_code) { %>
        <a href="http://www.beian.gov.cn/portal/registerSystemInfo?recordcode=<%- theme.footer.beian.police_code %>" target="_blank" rel="nofollow noopener"> | <%- theme.footer.beian.police_text %></a>
      <% } %>
    <% } %>
  </div>
</footer>

  <!-- SCRIPTS -->
  <%- js("https://cdn.jsdelivr.net/npm/mdbootstrap@4.8.7/js/jquery-3.4.1.min.js") %>
  <%- js("https://cdn.jsdelivr.net/npm/mdbootstrap@4.8.7/js/popper.min.js") %>
  <%- js("https://cdn.jsdelivr.net/npm/mdbootstrap@4.8.7/js/bootstrap.min.js") %>
  <%- js("https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.8.7/js/mdb.min.js") %>
  <%- js("js/main.js") %>
  <% if(is_post()){ %>
    <% if(theme.post.toc.enable){ %>
      <%- js("https://cdnjs.cloudflare.com/ajax/libs/tocbot/4.7.0/tocbot.min.js") %>
    <% } %>
    <%- js("js/post.js") %>
    <% if(theme.highlight.enable){ %>
      <%- js("https://cdn.jsdelivr.net/npm/code-prettify@0.1.0/src/prettify.min.js") %>
      <script>
        $(document).ready(function(){
          $('pre').addClass('prettyprint linenums');
          prettyPrint();
        })
      </script>
    <% } %>
  <% } %>
  <% if(theme.fun_features.typing.enable && is_post()){ %>
    <%- js("https://cdn.jsdelivr.net/npm/typed.js@2.0.9/lib/typed.min.js") %>
    <script>
        var typed = new Typed('#subtitle', {
          strings: [
            '  ',
            "<%- data.subtitle %>&nbsp;",
        ],
        cursorChar: "<%- theme.fun_features.typing.cursorChar %>",
        typeSpeed: <%- theme.fun_features.typing.typeSpeed %>,
        startDelay: 300, //开始之前延迟300毫秒
        loop: <%- theme.fun_features.typing.loop %>,
        });
        $(".typed-cursor").addClass("h2");
    </script>
  <% } %>
  <% if(theme.fun_features.anchorjs.enable){ %>
    <%- js("https://cdn.jsdelivr.net/npm/anchor-js@4.2.0/anchor.min.js") %>
    <script>
      anchors.options = {
        placement: "<%- theme.fun_features.anchorjs.placement %>",
        visible: "<%- theme.fun_features.anchorjs.visible %>",
        <% if(theme.fun_features.anchorjs.icon != ""){ %>
        icon: "<%- theme.fun_features.anchorjs.icon %>" 
        <% } %>
      };
      anchors.add(".post-content > <%- theme.fun_features.anchorjs.element %>");
    </script>
  <% } %>